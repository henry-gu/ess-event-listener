<!DOCTYPE html>
<html lang="en" dir="ltr">
  <%- include('partials/header') %>
  <body>
    <div class="container">
      <h1>Event</h1>
      <form action="/eventlistener" method="post">
        <div class="form-group">
          <label for="title">Title</label>
          <input type="text" class="form-control" id="title" name="title" required="true" />
        </div>
        <div class="form-group">
          <label for="payload">Payload</label>
          <textarea class="form-control" id="payload" name="payload" rows="4"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Pubish</button>
      </form>
    </div>
    <%- include('partials/footer') %>
  </body>
</html>
